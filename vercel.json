{
    "version": 2,
    "builds": [{
        "src": "officezenith/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb" }
    }],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/staticfiles/$1"
        },
        {
            "src": "/(.*)",
            "dest": "officezenith/wsgi.py"
        }
    ],
    "outputDirectory": "staticfiles"
}